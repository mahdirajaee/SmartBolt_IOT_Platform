{
    "service": {
      "id": "TimeSeriesDBConnector",
      "host": "localhost",
      "port": 8081
    },
    "catalog": {
      "url": "http://localhost:8080"
    },
    "influxdb": {
      "url": "http://localhost:8086",
      "token": "your_influxdb_token_here",
      "org": "organization",
      "bucket": "bucket"
    },
    "mqtt": {
      "broker": "localhost",
      "port": 1883,
      "username": "",
      "password": "",
      "client_id_prefix": "TimeSeriesDBConnector",
      "topics": [
        "/sensor/temperature",
        "/sensor/pressure"
      ]
    },
    "logging": {
      "level": "INFO",
      "format": "%(asctime)s - %(name)s - %(levelname)s - %(message)s"
    },
    "refresh_interval": 60
  }